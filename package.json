{
	"name": "cornucopia-website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"prebuild": "echo I run before the build script",
		"productionbuild": "vite build && npx svelte-sitemap --out-dir .vercel/output/static --domain https://cornucopia.dotnetlab.eu",
		"build": "vite build && npx svelte-sitemap --domain https://cornucopia.dotnetlab.eu",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.21.0",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@types/node": "^20.17.47",
		"svelte": "^4.2.19",
		"svelte-check": "^3.8.6",
		"svelte-sitemap": "^2.7.0",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"vite": "^5.4.19"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-static": "^3.0.8",
		"front-matter": "^4.0.2",
		"svelte-markdown": "^0.4.1",
		"vite-plugin-restart": "^0.4.2",
		"vite-plugin-static-copy": "^1.0.6"
	}
}
